        <pose>0 0 0 1.5708 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.07162</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
      </collision>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.5</mass>
        <inertia>
          <ixx>0.00103</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.00103</iyy>
          <iyz>0.0</iyz>
          <izz>0.00103</izz>
        </inertia>
      </inertial>
    </link>
    <!-- <gazebo reference="front_left_wheel_link">
