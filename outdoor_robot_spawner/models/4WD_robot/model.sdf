  <model name='4WD_robot'>
    <link name="base_footprint"></link>

    <link name="base_link">
      <pose relative_to='base_footprint'>0 0 0.1195 0 0 0</pose>
      <visual name="base_link_of_robot">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.445 0.2 0.129</size>
          </box>
        </geometry>
        <material>
          <script>
            <name>Gazebo/Yellow</name>
            <uri>file://media/materials/scripts/gazebo.material</uri>
          </script>
        </material>
      </visual>
      <collision name="base_link_of_robot_collision">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>0.445 0.2 0.129</size>
          </box>
        </geometry>
      </collision>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>12</mass>
        <inertia>
          <ixx>0.0566</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.2147</iyy>
          <iyz>0.0</iyz>
          <izz>0.238</izz>
        </inertia>
      </inertial>
    </link>
